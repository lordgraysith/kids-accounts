{
  "name": "kids-accounts",
  "version": "1.0.0",
  "scripts": {
    "client": "cd client && yarn start",
    "server": "node server/index.js",
    "server-dev": "supervisor server/index.js",
    "start": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "dev": "concurrently --kill-others-on-fail \"yarn server-dev\" \"yarn client\"",
    "deploy": "now && now alias && now ls kids-accounts && echo \"now scale <deployment> sfo 0 1\""
  },
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "apollo-server-express": "^1.3.4",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "config": "^1.30.0",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-tools": "^2.23.1",
    "lodash": "^4.17.5",
    "pg": "^7.4.1",
    "to-camel-case": "^1.0.0"
  },
  "devDependencies": {
    "supervisor": "^0.12.0"
  }
}
